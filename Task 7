Question 2

l = int(input("Enter the value for l  :") ) 
class shape():
    def __init__(self):
        pass
    def area(self):
        return 0
class square(shape):
    def __init__(self, l):
        shape.__init__(self)
        self.length = l
    def area(self):
        return self.length*self.length

square = square(l)
print(square.area())
